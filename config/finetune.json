{
    "data": {
        "x_load_path": "data/waveform/x.pt",
        "y_load_path": "data/waveform/y.pt",
        "profile_load_path": "data/waveform/profile.csv",
        "shuffle_channel_order": true,
        "shuffle_channel_num": false,
        "p_nan": 0,
        "batch_size": 256,
        "num_workers": 8
    },
    "model": {
        "class": "SCOST",
        "D": 256,
        "S": 20,
        "stride": 10,
        "C_max": 8,
        "L_max": 1024,
        "dropout": 0.05,
        "num_layers": 4,
        "nhead": 8,
        "dim_feedforward": 1024,
        "out_dim": 2,
        "freeze_embedding": true,
        "freeze_transformer": 2
    },
    "runner": {
        "class": "Finetune",
        "lr": 0.001,
        "step_size": 15,
        "gamma": 0.95
    },
    "trainer": {
        "max_epochs": 1000,
        "log_save_fold": "log/",
        "ckpt_save_fold": "ckpt/",
        "monitor": "loss/valid",
        "save_top_k": 5,
        "ckpt_load_path": "ckpt/pretrain/epoch=731-step=32940.ckpt",
        "resume": false
    }
}
